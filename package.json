{
  "name": "jag-dna-scientific-lims",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "debug:backend": "node --inspect backend/server.js",
    "debug:all": "concurrently \"npm run dev:debug\" \"npm run debug:backend\"",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop all",
    "restart": "pm2 restart all",
    "startup": "./start.sh",
    "status": "pm2 status",
    "logs": "pm2 logs",
    "build": "vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "seed:database": "node backend/scripts/simple-seed.js",
    "seed:comprehensive": "node backend/scripts/seed-database.js",
    "seed:fix-samples": "node backend/scripts/fix-samples.js",
    "test": "echo 'No tests specified yet' && exit 0",
    "lint": "eslint src --config .eslintrc.ci.js --max-warnings 100 || true",
    "lint:prod": "eslint src --config .eslintrc.ci.js --max-warnings 100 || true",
    "health-check": "curl -f http://localhost:3001/health || exit 1",
    "preview": "vite preview",
    "server": "node backend/server.js",
    "server:demo": "ENABLE_DEVOPS_FEATURES=true node backend/server.js",
    "server:quiet": "ENABLE_DEVOPS_FEATURES=false node backend/server.js",
    "server:full": "node backend/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:quiet\"",
    "dev:demo": "concurrently \"npm run dev\" \"npm run server:demo\"",
    "dev:frontend": "vite",
    "dev:backend": "node backend/server.js",
    "init-sheets": "node backend/scripts/initializeSheets.js",
    "electron": "wait-on http://localhost:5173 && electron .",
    "electron-dev": "concurrently \"npm run dev\" \"npm run server\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-pack": "electron-builder",
    "preelectron-pack": "npm run build",
    "build-electron": "npm run build && electron-builder",
    "build:electron:prod": "npm run build:prod && electron-builder --publish=never",
    "dist": "npm run build && electron-builder --publish=never",
    "clean": "rm -rf dist build",
    "clean:all": "npm run clean && rm -rf node_modules backend/node_modules",
    "analyze": "npm run build:prod && npx vite-bundle-analyzer dist",
    "optimize": "npm run lint:prod && npm run build:prod",
    "postinstall": "npm run postinstall:backend",
    "postinstall:backend": "cd backend && npm install",
    "audit:prod": "npm audit --audit-level moderate",
    "security:check": "npm audit && npm run audit:prod",
    "load-test": "node backend/scripts/runLoadTest.js",
    "load-test:light": "node backend/scripts/runLoadTest.js --concurrency 3 --duration 30",
    "load-test:heavy": "node backend/scripts/runLoadTest.js --concurrency 10 --duration 120",
    "devops:start": "npm run server",
    "devops:metrics": "curl -s http://localhost:3001/metrics | head -50",
    "devops:health": "curl -s http://localhost:3001/health | jq .",
    "devops:dashboard": "open http://localhost:3001/admin",
    "devops:performance-test": "curl -s http://localhost:3001/performance/slow && curl -s http://localhost:3001/performance/unreliable",
    "devops:memory-status": "curl -s http://localhost:3001/performance/memory-status | jq .",
    "devops:trigger-issues": "curl -X POST http://localhost:3001/performance/memory-leak -d '{\"size\":1000,\"iterations\":10}' -H 'Content-Type: application/json' && curl -X POST http://localhost:3001/performance/cpu-intensive -d '{\"duration\":5000}' -H 'Content-Type: application/json'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.7",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.3.0",
    "googleapis": "^129.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.522.0",
    "memory-usage": "^1.0.0",
    "node-cron": "^3.0.3",
    "node-pty": "^1.0.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "pidusage": "^3.0.2",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^3.0.0",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^6.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.10.3",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.5.1",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "vite": "^5.0.8",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.jagdnascientific.lims",
    "productName": "JAG DNA Scientific LIMS",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "public/electron.js",
      "public/preload.js",
      "backend/**/*",
      "!backend/node_modules/**/*",
      "!backend/scripts/**/*",
      "!backend/uploads/**/*",
      "!backend/reports/**/*",
      "!backend/**/*.log",
      "!backend/**/*.db*",
      "!backend/TestAnalysis.tar.gz",
      "!backend/NIST_STR_samples.html",
      "!data/**/*",
      "!external/**/*",
      "!workspace/**/*",
      "!scripts/**/*",
      "!docs/**/*",
      "!**/*.md",
      "!**/*.log"
    ],
    "mac": {
      "category": "public.app-category.medical",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "extraMetadata": {
      "main": "public/electron.js"
    }
  }
}
